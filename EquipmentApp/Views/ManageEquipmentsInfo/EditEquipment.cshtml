
@{
    ViewBag.Title = "Edit Equipment";
}
<table><tr><td height="10px"></td></tr></table>

<table cellpadding="4" cellspacing="4" width="70%" align="center" style="background-color: skyblue; border:solid 2px red; padding-top:20px;">
    <tr><td colspan="3" style="background-color:gray; font-size:18pt; font-weight:bold; height:30px; text-align:center;">Edit Equipment Information</td></tr>
    <tr>

        <td style="text-align:right;">Equipment ID</td>
        <td><input type="text" ng-model="Equipment.id" />  </td>
    </tr>
    <tr>

        <td style="text-align:right;">Equipment Name</td>
        <td><input type="text" ng-model="Equipment.equipmentName" />  </td>
    </tr>
    <tr>
        <td></td>
        <td>
            <input type="button" value="save" ng-click="save()" />
            <br />
            <div>{{error}}</div>
        </td>
    </tr>

</table>
