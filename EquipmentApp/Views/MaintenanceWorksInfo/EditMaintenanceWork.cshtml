
@{
    ViewBag.Title = "Edit Maintenance Work";
}
<table><tr><td height="10px"></td></tr></table>

<table cellpadding="4" cellspacing="4" width="70%" align="center" style="background-color: skyblue; border:solid 2px red; padding-top:20px;">
    <tr><td colspan="3" style="background-color:gray; font-size:18pt; font-weight:bold; height:30px; text-align:center;">Edit Maintenance Work Information</td></tr>
    <tr>

        <td style="text-align:right;">Maintenenace Work ID</td>
        <td><input type="text" ng-model="MaintenanceWork.id" />  </td>
    </tr>
    <tr>

        <td style="text-align:right;">Date</td>
        <td><input type="text" ng-model="MaintenanceWork.date" />  </td>
    </tr>
    <tr>

        <td style="text-align:right;">Time</td>
        <td><input type="text" ng-model="MaintenanceWork.time" />  </td>
    </tr>
    <tr>

        <td style="text-align:right;">Work Description</td>
        <td><input type="text" ng-model="MaintenanceWork.workDescription" />  </td>
    </tr>
    <tr>

        <td style="text-align:right;">Equipment Id</td>
        <td><input type="text" ng-model="MaintenanceWork.equipmentId" />  </td>
    </tr>
    <tr>
        <td style="text-align:right;">Time Taken</td>
        <td><input type="text" ng-model="MaintenanceWork.timeTaken" />  </td>
    </tr>
    <tr>
        <td></td>
        <td>
            <input type="button" value="save" ng-click="save()" />
            <br />
            <div>{{error}}</div>
        </td>
    </tr>

</table>
