@{
    ViewBag.Title = "Delete Maintenance Work";
}

<table><tr><td height="10px"></td></tr></table>

<table cellpadding="4" cellspacing="4" width="70%" align="center" style="background-color: skyblue; border:solid 2px red; padding-top:20px;">
    <tr><td colspan="3" style="background-color:gray; font-size:18pt; font-weight:bold; height:30px; text-align:center;">Delete Maintenance Information</td></tr>
    <tr>

        <td style="text-align:right;">Maintenance Work ID</td>
        <td>{{MaintenanceWork.id}}  </td>
    </tr>
    <tr>

        <td style="text-align:right;">Date</td>
        <td> {{MaintenanceWork.date}}  </td>
    </tr>
    <tr>

        <td style="text-align:right;">Time</td>
        <td> {{MaintenanceWork.time}}  </td>
    </tr>
    <tr>

        <td style="text-align:right;">Work Description</td>
        <td> {{MaintenanceWork.workDescription}}  </td>
    </tr>
    <tr>

        <td style="text-align:right;">Equipment</td>
        <td> {{MaintenanceWork.equipmentId}}  </td>
    </tr>
    <tr>

        <td style="text-align:right;">Time Taken</td>
        <td> {{MaintenanceWork.timeTaken}}  </td>
    </tr>
    <tr>
        <td></td>
        <td>
            <input type="button" value="Delete" ng-click="delete()" />
        </td>
    </tr>

</table>


